//-----------------------------------------------------------------------------
//                         GLOBAL Defination
//-----------------------------------------------------------------------------
#ifdef USE_GL
	#version 120
#else
	#version 100
	precision mediump int;
	precision highp float;
#endif
//-----------------------------------------------------------------------------
//                         GLOBAL PARAMETERS
//-----------------------------------------------------------------------------
uniform sampler2D DiffuseMap;
//-----------------------------------------------------------------------------
//                         Pixel Inputs
//-----------------------------------------------------------------------------
varying vec2 oUv;
//-----------------------------------------------------------------------------
// Function Name: main
// Function Desc: Pixel Program Entry point
//-----------------------------------------------------------------------------
void main()
{	
	gl_FragColor = texture2D(DiffuseMap, oUv);
}